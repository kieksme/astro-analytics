{
  "name": "astro-analytics",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "0.4.0",
  "publisher": "kieksme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kieksme/astro-analytics"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": ["Other"],
  "keywords": ["astro", "analytics", "ga4", "google-analytics", "markdown", "codelens"],
  "license": "GPL-3.0",
  "icon": "icon.png",
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:markdown",
    "onLanguage:astro",
    "onCommand:astro-analytics.showDashboard",
    "onCommand:astro-analytics.testConnection",
    "onCommand:astro-analytics.refresh",
    "onCommand:astro-analytics.configure",
    "onView:astroAnalytics.dashboard"
  ],
  "main": "./dist/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "icons": [
      {
        "id": "astro-analytics.analytics",
        "description": "%icon.analytics.description%",
        "default": {
          "light": "resources/icons/analytics.svg",
          "dark": "resources/icons/analytics.svg"
        }
      },
      {
        "id": "astro-analytics.bounce-good",
        "description": "%icon.bounceGood.description%",
        "default": {
          "light": "resources/icons/bounce-good.svg",
          "dark": "resources/icons/bounce-good.svg"
        }
      },
      {
        "id": "astro-analytics.bounce-warning",
        "description": "%icon.bounceWarning.description%",
        "default": {
          "light": "resources/icons/bounce-warning.svg",
          "dark": "resources/icons/bounce-warning.svg"
        }
      },
      {
        "id": "astro-analytics.bounce-high",
        "description": "%icon.bounceHigh.description%",
        "default": {
          "light": "resources/icons/bounce-high.svg",
          "dark": "resources/icons/bounce-high.svg"
        }
      },
      {
        "id": "astro-analytics.bounce-critical",
        "description": "%icon.bounceCritical.description%",
        "default": {
          "light": "resources/icons/bounce-critical.svg",
          "dark": "resources/icons/bounce-critical.svg"
        }
      }
    ],
    "commands": [
      {
        "command": "astro-analytics.refresh",
        "title": "%command.refresh.title%",
        "icon": "$(sync)",
        "category": "%command.category%"
      },
      {
        "command": "astro-analytics.testConnection",
        "title": "%command.testConnection.title%",
        "icon": "$(debug-start)",
        "category": "%command.category%"
      },
      {
        "command": "astro-analytics.configure",
        "title": "%command.configure.title%",
        "category": "%command.category%"
      },
      {
        "command": "astro-analytics.showDashboard",
        "title": "%command.showDashboard.title%",
        "icon": "$(astro-analytics.analytics)",
        "category": "%command.category%"
      }
    ],
    "configuration": {
      "title": "%config.title%",
      "properties": {
        "astroAnalytics.propertyId": {
          "type": "string",
          "default": "",
          "description": "%config.propertyId%"
        },
        "astroAnalytics.credentialsPath": {
          "type": "string",
          "default": "",
          "description": "%config.credentialsPath%"
        },
        "astroAnalytics.lookbackDays": {
          "type": "number",
          "default": 30,
          "description": "%config.lookbackDays%"
        },
        "astroAnalytics.contentRoot": {
          "type": "string",
          "default": "src/content",
          "description": "%config.contentRoot%"
        },
        "astroAnalytics.pagesRoot": {
          "type": "string",
          "default": "src/pages",
          "description": "%config.pagesRoot%"
        }
      }
    },
    "menus": {
      "editor/title": [
        {
          "command": "astro-analytics.refresh",
          "when": "resourceExtname == .md || resourceExtname == .mdx || resourceExtname == .astro",
          "group": "navigation"
        },
        {
          "command": "astro-analytics.testConnection",
          "when": "resourceExtname == .md || resourceExtname == .mdx || resourceExtname == .astro",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "astroAnalytics",
          "title": "%viewContainer.title%",
          "icon": "resources/icons/analytics.svg"
        }
      ]
    },
    "views": {
      "astroAnalytics": [
        {
          "id": "astroAnalytics.dashboard",
          "name": "%view.dashboard.name%"
        }
      ]
    }
  },
  "scripts": {
    "build": "pnpm test && pnpm compile && pnpm package",
    "compile": "tsc -p ./ && pnpm run bundle",
    "compile:all": "pnpm run compile && pnpm run compile:tests",
    "compile:tests": "tsc -p tsconfig.test.json",
    "watch": "tsc -watch -p ./",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "pnpm run compile && pnpm run compile:tests && vscode-test",
    "vscode:prepublish": "pnpm run compile",
    "bundle": "esbuild src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node",
    "package": "pnpm run compile && pnpx vsce package --no-dependencies --allow-missing-repository"
  },
  "dependencies": {
    "google-auth-library": "^9.6.3",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.11",
    "@types/mocha": "^10.0.0",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.3.9",
    "esbuild": "^0.24.0",
    "glob": "^11.0.0",
    "mocha": "^10.8.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0",
    "@vscode/vsce": "^2.24.0"
  }
}
